import BaseView from '../baseview';
import RatingBlock from '../../blocks/ratingblock/rating';


export default class RatingView extends BaseView {
    constructor(parent) {
        super(parent);

        this.blockName = 'rating';
        this.block = new RatingBlock(['user1', 'user2', 'user3', 'user4']);

        this.eventBus.on('main-block:rating', () => {
            this.create()
        });
    }

}